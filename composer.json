{
    "name": "parsidev/json",
    "description": "JSON decoder/encoder for Laravel 5.6",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Reza",
            "email": "info@parsidev.ir"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "6.0.* | 6.2.*",
        "justinrainbow/json-schema": "^5.2",
        "seld/jsonlint": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Parsidev\\Json": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parsidev\\Json\\JsonServiceProvider"
            ],
            "aliases": {
                "ParsJSON": "Parsidev\\Json\\Facades\\JsonFacade"
            }
        }
},
    "minimum-stability": "stable"
}
